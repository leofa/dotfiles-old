}

arches() {
initializeANSI()
{
  esc=""

  redf="${esc}[31m";     greenf="${esc}[32m"
  yellowf="${esc}[33m"   bluef="${esc}[34m";   
  cyanf="${esc}[36m";    purplef="${esc}[35m"   
                      
  boldon="${esc}[1m";    
  reset="${esc}[0m"
}

initializeANSI

cat << EOF 
  
${boldon}${redf}        â–       ${boldon}${greenf}        â–       ${boldon}${yellowf}        â–      ${boldon}${bluef}         â–        ${boldon}${purplef}       â–       ${boldon}${cyanf}        â–    ${reset}
${boldon}${redf}       â– â– â–      ${boldon}${greenf}       â– â– â–      ${boldon}${yellowf}       â– â– â–     ${boldon}${bluef}        â– â– â–       ${boldon}${purplef}      â– â– â–      ${boldon}${cyanf}       â– â– â–   ${reset}
${boldon}${redf}      â– â– â– â– â–     ${boldon}${greenf}      â– â– â– â– â–     ${boldon}${yellowf}      â– â– â– â– â–    ${boldon}${bluef}       â– â– â– â– â–      ${boldon}${purplef}     â– â– â– â– â–     ${boldon}${cyanf}      â– â– â– â– â–  ${reset}
${redf}     â– (   )â–    ${greenf}     â– (   )â–    ${yellowf}     â– (   )â–    ${bluef}     â– (   )â–     ${purplef}    â– (   )â–    ${cyanf}     â– (   )â–    ${reset}
${redf}    â– â– â– â–  â– â– â– â–   ${greenf}    â– â– â– â–  â– â– â– â–   ${yellowf}    â– â– â– â–  â– â– â– â–   ${bluef}    â– â– â– â–  â– â– â– â–    ${purplef}   â– â– â– â–  â– â– â– â–   ${cyanf}    â– â– â– â–  â– â– â– â–   ${reset}
${redf}   â– â–        â– â–  ${greenf}   â– â–        â– â–  ${yellowf}   â– â–        â– â–  ${bluef}   â– â–        â– â–   ${purplef}  â– â–        â– â–  ${cyanf}   â– â–        â– â–  ${reset}
 
EOF
}


archlogo() {

L="$(tput sgr0; tput setaf 0)"
LB="$(tput sgr0; tput setab 0)"
l="$(tput bold ; tput setaf 0)"
w="$(tput sgr0; tput setaf 7)"
WB="$(tput sgr0; tput setab 7)"
W="$(tput bold ; tput setaf 7)"
R="$(tput sgr0; tput setaf 1)"
RB="$(tput sgr0; tput setab 1)"
r="$(tput bold ; tput setaf 1)"
G="$(tput sgr0; tput setaf 2)"
GB="$(tput sgr0; tput setab 2)"
g="$(tput bold ; tput setaf 2)"
Y="$(tput sgr0; tput setaf 3)"
YB="$(tput sgr0; tput setab 3)"
y="$(tput bold ; tput setaf 3)"
B="$(tput sgr0; tput setaf 4)"
BB="$(tput sgr0; tput setab 4)"
b="$(tput bold ; tput setaf 4)"
P="$(tput sgr0; tput setaf 5)"
PB="$(tput sgr0; tput setab 5)"
p="$(tput bold ; tput setaf 5)"
C="$(tput sgr0; tput setaf 6)"
CB="$(tput sgr0; tput setab 6)"
c="$(tput bold ; tput setaf 6)"
N="$(tput sgr0)"

echo "$WB                                                                                "
echo " $N                                                                              $WB "
echo " $N           $CB $N            $WB                                                     $N $WB "
echo " $N          $CB   $N           $WB $N$CB                                                   $WB $N $WB "
echo " $N         $CB     $N          $WB $N$CB                                                   $WB $N $WB "
echo " $N        $CB       $N         $WB $N$CB    $N  $CB  $N    $CB  $N    $CB $N $CB  $N $CB  $WB $CB    $WB $CB $WB $CB   $WB $CB $WB $CB  $WB $CB $WB $CB   $WB $CB   $WB $N $WB "
echo " $N       $CB         $N        $WB $N$CB   $N $CB  $N $CB $N $CB   $N $CB $N $CB    $N $CB  $N $CB  $WB $CB    $WB $CB $WB $WB $CB  $WB $CB $WB $CB  $WB $CB  $WB $CB $WB $CB    $WB $N $WB "
echo " $N      $CB           $N       $WB $N$CB   $N    $CB $N    $CB  $N $CB    $N    $CB  $WB $CB    $WB $CB $WB $CB $WB $CB $WB $CB $WB $CB  $WB $CB   $WB $CB     $WB $N $WB "
echo " $N     $CB     $N   $CB     $N      $WB $N$CB   $N $CB  $N $CB $N $CB   $N $CB $N $CB    $N $CB  $N $CB  $WB $CB    $WB $CB $WB $CB  $WB $WB $CB $WB $CB  $WB $CB  $WB $CB $WB $CB    $WB $N $WB "
echo " $N    $CB     $N     $CB     $N     $WB $N$CB   $N $CB  $N $CB $N $CB   $N $CB $N    $CB $N $CB  $N $CB  $WB $WB $WB $WB $CB $WB $CB $WB $CB   $WB $CB  $WB $WB $CB  $WB $CB   $WB $CB   $WB $N $WB "
echo " $N   $CB      $N     $CB      $N    $WB $N$CB                                                   $WB $N $WB "
echo " $N  $CB        $N   $CB        $N   $WB $N$CB                                                   $WB $N $WB "
echo " $N $CB   $N               $CB   $N  $WB                                                     $N $WB "
echo " $N                                                                              $WB "
echo "                                                                                $N"
echo 
}

tux() {
initializeANSI()
{
  esc=""

  blackf="${esc}[30m";   redf="${esc}[31m";    greenf="${esc}[32m"
  yellowf="${esc}[33m"   bluef="${esc}[34m";   purplef="${esc}[35m"
  cyanf="${esc}[36m";    whitef="${esc}[37m"
  
  blackb="${esc}[40m";   redb="${esc}[41m";    greenb="${esc}[42m"
  yellowb="${esc}[43m"   blueb="${esc}[44m";   purpleb="${esc}[45m"
  cyanb="${esc}[46m";    whiteb="${esc}[47m"

  boldon="${esc}[1m";    boldoff="${esc}[22m"
  italicson="${esc}[3m"; italicsoff="${esc}[23m"
  ulon="${esc}[4m";      uloff="${esc}[24m"
  invon="${esc}[7m";     invoff="${esc}[27m"

  reset="${esc}[0m"
}

initializeANSI

tuxbg=${cyanb}     # color behind tux
tuxfg=${blackf}     # tux line color

cat << EOF

${tuxfg}${tuxbg}
                                                     
                      â–„â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„                        
                     â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ                       
                    ${whiteb}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–ˆâ–ˆ${tuxbg}                      
                   ${whiteb}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–„â–ˆâ–ˆ${tuxbg}                     
                   ${whiteb}â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}                     
                   ${whiteb}â–ˆâ–ˆâ–ˆâ–ˆ â–ˆ â–ˆâ–ˆ â–ˆâ–ˆâ–ˆ${tuxbg}                     
                   ${yellowb}â–ˆ         â–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}                    
                   ${yellowb}â–ˆ       â–„ â–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}                    
                   ${yellowb}â–ˆâ–ˆâ–ˆâ–€â–€â–€â–€â–€â–„${whiteb}â–€â–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}                    
                   ${whiteb}â–ˆâ–ˆâ–€â–€â–€â–€â–€â–€   â–ˆâ–ˆâ–ˆ${tuxbg}â–„                   
                 â–„â–ˆ${whiteb}â–€          â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}                  
                ${whiteb}â–ˆâ–ˆâ–ˆ           â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}                 
               ${whiteb}â–ˆâ–ˆâ–ˆ             â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}                
              ${whiteb}â–ˆâ–€â–ˆâ–ˆ              â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}               
              ${whiteb}â–ˆ â–ˆ               â–ˆ â–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}               
              ${whiteb}â–ˆ â–ˆ               â–ˆâ–ˆ â–ˆâ–ˆâ–ˆ${tuxbg}               
             ${whiteb}â–ˆâ–ˆ â–€               â–ˆâ–€ â–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}              
             ${whiteb}â–ˆâ–ˆâ–ˆ                   â–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}              
            ${whiteb}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆ               â–ˆâ–ˆâ–ˆ â–ˆâ–ˆâ–ˆ${tuxbg}              
            ${yellowb}â–ˆâ–€â–€â–ˆâ–ˆâ–ˆ${whiteb}             â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${tuxbg}             
           â–„${yellowb}â–ˆ   â–ˆâ–ˆâ–ˆ${whiteb}           â–ˆ${yellowb}â–€ â–ˆâ–ˆâ–ˆâ–ˆ  â–€â–ˆ${tuxbg}            
         â–„â–ˆ${yellowb}â–€     â–ˆâ–ˆâ–ˆâ–ˆ${whiteb}         â–ˆ${yellowb}   â–€     â–ˆ${tuxbg}            
        â–ˆ${yellowb}         â–ˆâ–ˆâ–ˆâ–ˆ${whiteb}     â–ˆ  â–ˆ${yellowb}         â–ˆâ–ˆ${tuxbg}           
         â–ˆ${yellowb}         â–ˆâ–ˆ${whiteb}       â–ˆ â–ˆ${yellowb}          â–€â–ˆ${tuxbg}          
        â–ˆ${yellowb}           â–ˆ${whiteb}      â–ˆ  â–ˆ${yellowb}          â–ˆ${tuxbg}           
        â–ˆ${yellowb}           â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${yellowb}        â–„${tuxbg}â–€            
         â–ˆ${yellowb}â–„         â–ˆ${tuxbg} â–€â–€â–€â–€â–€â–€â–€ â–ˆ${yellowb}      â–„${tuxbg}â–€              
           â–€â–€â–€â–€â–€${yellowb}â–„â–„â–„â–ˆ${tuxbg}â–€         â–€${yellowb}â–„    â–ˆ${tuxbg}                
                                â–€â–€â–€â–€${tuxbg}                 
${reset}
EOF
}

case $1 in
  -c) colors ;;
  -i) invaders ;;
  -p) pacman ;;
  -a) arches ;;
  -t) tux ;;
  -l) archlogo ;;
  -la) 
      archlogo
      tux 
      colors 
      invaders 
      pacman 
      arches ;;
  -h) usage ;;
  *) colors ;;
esac
